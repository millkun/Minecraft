      const currentRunes = (currentRound - 1) * 31;
      bot.chat(`&8-‚ñë-‚ñë-‚ñë-‚ñë-‚ñë-‚ñë-‚ñë-‚ñë-‚ñë-‚ñë-‚ñë-‚ñë-‚ñë-‚ñë- &6&l[&e&l–ê—Ñ–∫-–õ–æ–±–±–∏&6&l] &a&l${currentPlayer} &b–∑–∞—Ä–∞–±–æ—Ç–∞–ª &e${score} &b–æ—á–∫–∞(-–æ–≤) –≤ –∫–æ–º–±–∏–Ω–∞—Ü–∏–∏ –∏ –ø–æ–ª—É—á–∏–ª &e„Å≤&a${currentRunes}`);
      sendLogMessage(`${currentPlayer} —Å—ã–≥—Ä–∞–ª –≤ –∫–æ–º–±–∏–Ω–∞—Ü–∏—é. –ü—Ä–æ–π–¥–µ–Ω–æ —Ä–∞—É–Ω–¥–æ–≤ - ${currentRound - 1}, –û—á–∫–∏ - ${score}. #–∫–æ–º–±–∏–Ω–∞—Ü–∏—è`);
      resetGame();
      bot.chat(`/rg f combination use deny`);
    } else {
      console.log(`${currentPlayer} complete round ${currentRound}!`);
      playerLog.gamesPlayed++;
      playerLog.runes += 31;
      currentRound++;
      startNewRound();
    }
